# 应用配置
APP_ENV=development
APP_DEBUG=true
APP_KEY=

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8888

# 数据库配置
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# Redis配置
REDIS_ENABLED=false
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_PREFIX=
REDIS_TIMEOUT=5

# 日志配置
LOG_CHANNEL=stack
LOG_LEVEL=debug

# 邮件配置
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME=

# 文件存储配置
# ========

# 存储驱动 (可选值: local, oss, s3)
STORAGE_DRIVER=local

# 本地存储配置 (当 STORAGE_DRIVER=local 时)
LOCAL_STORAGE_PATH=./storage/uploads
LOCAL_STORAGE_URL=/uploads

# 阿里云OSS配置 (当 STORAGE_DRIVER=oss 时)
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET_NAME=your_oss_bucket_name
OSS_BASE_URL=https://your-oss-cdn-domain.com

# AWS S3配置 (当 STORAGE_DRIVER=s3 时)
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your_s3_access_key_id
S3_SECRET_ACCESS_KEY=your_s3_secret_access_key
S3_BUCKET_NAME=your_s3_bucket_name
S3_BASE_URL=https://your-cloudfront-domain.com

# AI 配置
# ========

# 默认 AI 提供商
AI_DEFAULT_PROVIDER=openai

# OpenAI 配置
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2000

# Anthropic 配置
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_API_BASE=https://api.anthropic.com
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_TEMPERATURE=0.7
ANTHROPIC_MAX_TOKENS=2000

# 豆包 (Doubao) 配置
DOUBAO_API_KEY=your-doubao-api-key-here
DOUBAO_API_BASE=https://ark.cn-beijing.volces.com/api/v3
DOUBAO_MODEL=ep-20240611085556-2v8mg
DOUBAO_TEMPERATURE=0.7
DOUBAO_MAX_TOKENS=2000

# 通义千问 (Qianwen) 配置
QIANWEN_API_KEY=your-qianwen-api-key-here
QIANWEN_API_BASE=https://dashscope.aliyuncs.com/api/v1
QIANWEN_MODEL=qwen-max
QIANWEN_TEMPERATURE=0.7
QIANWEN_MAX_TOKENS=2000

# ChatGLM 配置
CHATGLM_API_KEY=your-chatglm-api-key-here
CHATGLM_API_BASE=https://open.bigmodel.cn/api/paas/v4
CHATGLM_MODEL=glm-4
CHATGLM_TEMPERATURE=0.7
CHATGLM_MAX_TOKENS=2000

# Baichuan 配置
BAICHUAN_API_KEY=your-baichuan-api-key-here
BAICHUAN_API_BASE=https://api.baichuan-ai.com/v1
BAICHUAN_MODEL=Baichuan2-Turbo
BAICHUAN_TEMPERATURE=0.7
BAICHUAN_MAX_TOKENS=2000

# MiniMax 配置
MINIMAX_API_KEY=your-minimax-api-key-here
MINIMAX_API_SECRET=your-minimax-api-secret-here
MINIMAX_API_BASE=https://api.minimax.chat/v1
MINIMAX_MODEL=abab6.5-chat
MINIMAX_TEMPERATURE=0.7
MINIMAX_MAX_TOKENS=2000

# AI 全局设置
AI_REQUEST_TIMEOUT=30
AI_MAX_RETRIES=3
AI_RETRY_DELAY=1
AI_RATE_LIMIT_ENABLED=true
AI_RATE_LIMIT_RPM=60
AI_CONVERSATION_MAX_HISTORY=50
AI_EMBEDDING_BATCH_SIZE=100

# AI 功能开关
AI_CHAT_ENABLED=true
AI_COMPLETION_ENABLED=true
AI_EMBEDDING_ENABLED=true
AI_STREAMING_ENABLED=true

# AI 安全设置
AI_CONTENT_FILTER_ENABLED=true
AI_API_KEY_ENCRYPTION=false
AI_AUDIT_LOG_ENABLED=true

# AI 缓存设置
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
AI_CACHE_MAX_SIZE=1000

# AI 监控设置
AI_METRICS_ENABLED=true
AI_HEALTH_CHECK_ENABLED=true
AI_PERFORMANCE_LOGGING=true